# Supabase Configuration
# Copy this file to .env and fill in your actual values
# Get these values from your Supabase project settings -> API
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Receipt Scanner Configuration (Optional)
# Cloudflare Worker URL for R2 direct upload (PUT ?file=filename.jpg)
VITE_R2_PRESIGNED_API=https://r2-signer.aiqswings87.workers.dev

# n8n Webhook URL for AI receipt analysis
VITE_N8N_WEBHOOK_URL=https://your-n8n-instance.app/webhook/receipt-scanner

# Demo Mode (set to 'true' to use mock data without Supabase)
VITE_USE_DEMO=false
